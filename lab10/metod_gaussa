#include <stdio.h>
#include <math.h>
int main(){
    int i,j;
   float mat[3][3]={{2.5,-3.12,-4.03},{0.61,0.71,-0.05},{-1.03,-2.05,0.87}};
   float matsv[1][3]={-7.5,0.44,-1.16};
   float matrash[3][4]={{2.5,-3.12,-4.03,-7.5},{0.61,0.71,-0.05,0.44},{-1.03,-2.05,0.87,-1.16}};
   float matrash1[3][4];
   for(i=0;i<3;i++){
       for(j=0;j<4;j++){
       matrash1[i][j] = matrash[i][j]/matrash[0][0];
            printf("el[%d][%d]=%f ",i,j,matrash1[i][j]);
       }
       printf("\n");
   }


return 0;
 }
